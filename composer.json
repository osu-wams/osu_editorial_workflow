{
  "name": "osu-wams/osu_editorial_workflow",
  "description": "Provides support for Editorial Workflows of content.",
  "minimum-stability": "stable",
  "type": "drupal-custom-module",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matthew Brabham",
      "email": "matthew.brabham@oregonstate.edu"
    }
  ],
  "scripts": {
    "cs-checkstyle": "phpcs -q --report=checkstyle",
    "phpcs": "phpcs",
    "phpcbf": "phpcbf",
    "yaml-lint": "yaml-lint *.yml"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0",
    "squizlabs/php_codesniffer": "^3.11",
    "drupal/coder": "^8.3"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
